<div style={{ border: '1px solid #d9d9d9' }}>
  <div class="core-l-list-page">
    <div class="core-l-list-page__main">
      <div class="core-l-list-page__banner">
        <div class="core-padding-lg" style={{ background: '#f5f5f5' }}>
          Work Order Contracts Viewer
        </div>
      </div>
      <div class="core-l-list-page__tool-header">
        <div class="core-padding-lg" style={{ background: '#f5f5f5' }}>
          Work Order Contracts
        </div>
      </div>
      <div class="core-l-list-page__banner">
        <div class="core-padding-lg" style={{ background: '#f5f5f5' }}>
          Bottom Banner
        </div>
      </div>
      <div class="core-l-list-page__content-controls">
        <div class="core-l-list-page__content-controls__control core-l-list-page__content-controls__control--search">
          <div class="core-padding-lg" style={{ background: '#f5f5f5' }}>
            Origin ID Search
          </div>
        </div>
      <div class="core-l-list-page__body">
        <div class="core-l-list-page__body__content">
          <div class="core-padding-lg" style={{ background: '#f5f5f5', flex: 1 }}>
            <div class="core-card core-card--level-30">
              <div class="core-table sortable">
                <table class="core-table__table sortable">
                  <thead class="core-table__head">
                    <tr class="core-table__head-row">
                      <th class="core-table__th">
                        <div class="core-table__th__label">
                          Title
                        </div>
                      </th>
                      <th class="core-table__th core-table__th--sortable core-table__th--sortable-asc">
                        <div class="core-table__th__label">
                          Number
                        </div>
                      </th>
                      <th class="core-table__th core-table__th--sortable">
                        <div class="core-table__th__label">
                          Origin ID
                        </div>
                      </th>
                      <th class="core-table__th core-table__th--sortable">
                        <div class="core-table__th__label">
                          Origin Data
                        </div>
                      </th>
                      <th class="core-table__th core-table__th--sortable">
                        <div class="core-table__th__label">
                          Origin Code
                        </div>
                      </th>
                    </tr>
                  </thead>

                  <tbody class="core-table__body">
                    <% @work_order_contracts.each do |contract| %>
                    <tr class="core-table__body-row">
                      <td class="core-table__td">
                        <label class="core-table__text-cell">
                          <%= contract['title'] %>
                        </label>
                      </td>
                      <td class="core-table__td">
                        <label class="core-table__text-cell">
                          <%= contract['number'] %>
                        </label>
                      </td>
                      <td class="core-table__td">
                        <label class="core-table__text-cell">
                          <%= contract['origin_id'] %>
                        </label>
                      </td>
                      <td class="core-table__td">
                        <label class="core-table__text-cell">
                          <%= contract['origin_data'] %>
                        </label>
                      </td>
                      <td class="core-table__td">
                        <label class="core-table__text-cell">
                          <%= contract['origin_code'] %>
                        </label>
                      </td>
                    </tr>
                    <% end %>
                  </tbody>

                  <tfoot>

                  </tfoot>
                </table>
              </div>
            </div>
          </div>
        </div>
      </div>
      <div class="core-l-list-page__content-controls">
        <div class="core-l-list-page__content-controls__control">
          <div class="core-padding-lg" style={{ background: '#f5f5f5' }}>
            Bulk Actions
          </div>
        </div>
        <div class="core-l-list-page__content-controls__control core-l-list-page__content-controls__control--align-right">
          <div class="core-padding-xs" style={{ background: '#f5f5f5' }}>
            Pagination
          </div>
        </div>
      </div>
    </div>
  </div>
</div>